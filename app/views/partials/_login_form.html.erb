<%= simple_form_for(:session, url: login_path) do |f| %>
  <%= f.input :email,autofocus: true, :placeholder => "your email here", class: "placeholder" %>
  <%= f.input :password, :placeholder => "Password" %> 
  <%= f.submit "Log in", class: "btn", data: { disable_with: "Logging in..." } %>
<% end %>